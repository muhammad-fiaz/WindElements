{
  "name": "windelements",
  "version": "0.0.1",
  "description": "Production-ready UI components for Vanilla JS/TS - like shadcn/ui but for vanilla JavaScript",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "windelements": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src/components",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun run build:lib && bun run build:cli",
    "build:lib": "tsc --project tsconfig.build.json",
    "build:cli": "tsc --project tsconfig.cli.json",
    "prepublishOnly": "bun run build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "components",
    "ui",
    "vanilla-js",
    "typescript",
    "tailwindcss",
    "component-library",
    "shadcn"
  ],
  "author": "Muhammad Fiaz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/muhammad-fiaz/WindElements.git"
  },
  "homepage": "https://muhammad-fiaz.github.io/WindElements",
  "bugs": {
    "url": "https://github.com/muhammad-fiaz/WindElements/issues"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.1",
    "@types/prompts": "^2.4.9",
    "esbuild": "^0.27.0",
    "sass": "^1.94.0",
    "terser": "^5.44.1",
    "typescript": "~5.9.3",
    "vitepress": "^1.6.4",
    "vue": "^3.5.24"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "fs-extra": "^11.3.2",
    "ora": "^9.0.0",
    "oxc-minify": "^0.97.0",
    "prompts": "^2.4.2",
    "tailwindcss": "^4.1.17"
  },
  "peerDependencies": {
    "tailwindcss": "^4.0.0"
  }
}
